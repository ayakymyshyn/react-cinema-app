{
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "jsx-a11y"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-param-reassign": 0,
        "no-confusing-arrow": 0,
        "import/prefer-default-export": 0
    }
}